/**app.wxss**/
.cell-item{
  display: flex;
  align-items: center;
  height: 100rpx;
  font-size:28rpx;
  border-bottom: 1rpx solid #eee;
  position: relative;
}
.cell-item image{
  width: 54rpx;
  height: 54rpx;
  margin: 10rpx 40rpx;
}
.cell-item::after{
  content: '';
  width: 20rpx;
  height: 20rpx;
  display: inline-block;
  border-top: 1rpx solid #000;
  border-right: 1rpx solid #000;
  position: absolute;
  right: 20rpx;
  transform: rotate(45deg);
}
